{
  "name": "meldekort-mikrofrontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(ts|tsx)\""
  },
  "dependencies": {
    "@navikt/aksel-icons": "6.1.0",
    "@navikt/ds-react": "6.1.0",
    "@navikt/nav-dekoratoren-moduler": "^2.1.5",
    "dayjs": "1.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.6.5",
    "swr": "2.2.5"
  },
  "devDependencies": {
    "@testing-library/react": "15.0.2",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/react-modal": "3.16.3",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.1",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "prettier": "^3.2.5",
    "pretty-quick": "4.0.0",
    "rollup-plugin-import-map": "^2.2.2",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-css-injected-by-js": "3.5.0",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-mock": "3.0.2",
    "vitest": "1.5.0"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.js --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  }
}
