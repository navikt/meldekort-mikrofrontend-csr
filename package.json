{
  "name": "meldekort-mikrofrontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(ts|tsx)\""
  },
  "dependencies": {
    "@navikt/aksel-icons": "4.1.7",
    "@navikt/ds-react": "4.1.7",
    "@navikt/nav-dekoratoren-moduler": "^1.9.0",
    "dayjs": "1.11.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.4.4",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "3.16.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@vitejs/plugin-react": "4.0.4",
    "eslint": "8.22.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "husky": "7.0.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.0.3",
    "prettier": "^3.0.1",
    "pretty-quick": "3.1.3",
    "rollup-plugin-import-map": "2.2.2",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-css-injected-by-js": "3.3.0",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-mock": "2.9.6",
    "vitest": "0.34.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c .eslintrc.pre-commit.js --fix",
      "prettier --cache --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write"
    ]
  }
}
