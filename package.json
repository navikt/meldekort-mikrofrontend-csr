    "react-dom": "18.3.1",
    "react-intl": "7.1.6",
    "swr": "2.3.3"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-modal": "3.16.3",
    "@typescript-eslint/eslint-plugin": "8.26.1",
